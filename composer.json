{
      "name":"islandora/islandora-project",
      "type":"project",
      "description":"Project template for Islandora sites.",
      "license":"GPL-2.0-or-later",
      "require":{
        "cweagans/composer-patches":"^1.6.0",
        "drupal/core-composer-scaffold":"*",
        "library/masonry":"*",
        "oomphinc/composer-installers-extender":"^1.1 || ^2",
        "wikimedia/composer-merge-plugin":"2.x-dev"
      },
      "require-dev":{
        "drush/drush":">=9.7",
        "phpspec/prophecy-phpunit":"^2"
      },
      "config":{
        "process-timeout":0
      },
      "extra":{
        "composer-exit-on-patch-failure":true,
        "drupal-scaffold":{
          "locations":{
            "web-root":"web/"
          }
        },
        "enable-patching":true,
        "installer-paths":{
          "web/core":[
            "type:drupal-core"
          ],
          "web/libraries/{$name}":[
            "type:drupal-library",
            "type:bower-asset",
            "type:npm-asset"
          ],
          "web/modules/contrib/{$name}":[
            "type:drupal-module"
          ],
          "web/profiles/contrib/{$name}":[
            "type:drupal-profile"
          ],
          "web/themes/contrib/{$name}":[
            "type:drupal-theme"
          ],
          "web/themes/custom/{$name}":[
            "type:drupal-theme-custom"
          ],
          "drush/contrib/{$name}":[
            "type:drupal-drush"
          ]
        },
        "installer-types":[
          "bower-asset",
          "npm-asset"
        ],
        "patchLevel":{
          "drupal/core":"-p2"
        },
        "merge-plugin":{
          "include":[
            "modules/contrib/masonry/composer.libraries.json"
          ]
        }
      },
      "repositories":{
        "drupal":{
          "type":"composer",
          "url":"https://packages.drupal.org/8"
        },
        "assets":{
          "type":"composer",
          "url":"https://asset-packagist.org"
        },
        "masonry":{
          "type":"package",
          "package":{
            "type":"drupal-library",
            "name":"library/masonry",
            "version":"3.3.2",
            "dist":{
              "type":"zip",
              "url":"https://github.com/desandro/masonry/archive/v3.3.2.zip"
            }
          }
        },
        "mjordan":{
          "type":"package",
          "package":{
            "name":"mjordan/islandora_workbench_integration",
            "version":"1.0",
            "type":"drupal-module",
            "source":{
              "url":"https://github.com/mjordan/islandora_workbench_integration.git",
              "type":"git",
              "reference":"main"
            }
          }

        },
        "pdfjs":{
          "type":"package",
          "package":{
            "name":"library/pdf.js",
            "version":"2.4.456",
            "type":"drupal-library",
            "dist":{
              "url":"https://github.com/mozilla/pdf.js/releases/download/v2.4.456/pdfjs-2.4.456-dist.zip",
              "type":"zip"
            }
          }
        },
        "openseadragon":{
          "type":"package",
          "package":{
            "name":"library/openseadragon",
            "version":"2.4.2",
            "type":"drupal-library",
            "dist":{
              "url":"https://github.com/openseadragon/openseadragon/releases/download/v2.4.2/openseadragon-bin-2.4.2.zip",
              "type":"zip"
            }
          }
        },
        "islandora_profile": {
          "type": "github",
          "url": "https://github.com/roblib/islandora_profile"
        }

    },
    "minimum-stability":"dev",
    "prefer-stable":false,
    "scripts":{
      "post-create-project-cmd":"rm -r -f .travis.yml .travis-ci",
      "quick-start":[
        "composer install",
        "php web/core/scripts/drupal quick-start lightning --no-interaction"
      ]
    }

}
