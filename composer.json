{
      "name":"islandora/islandora-project",
      "type":"project",
      "description":"Project template for Islandora sites.",
      "license":"GPL-2.0-or-later",
      "require":{
        "cweagans/composer-patches":"^1.6.0",
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^2.0",
        "drupal/content_browser": "^1.0@alpha",
        "drupal/core-composer-scaffold":"*",
        "drupal/facets": "^1.6",
        "drupal/matomo": "^1.7",
        "drupal/pdf": "*",
        "drupal/rdfui": "^1.0-beta1",
        "drupal/rest_oai_pmh": "^1.0@beta",
        "drupal/restui": "^1.16",
        "drupal/search_api_solr": "^4.1",
        "drupal/transliterate_filenames": "^1.3",
        "easyrdf/easyrdf": "*",
        "islandora-rdm/islandora_fits": "dev-8.x-1.x",
        "islandora/islandora": "dev-8.x-1.x",
        "islandora/controlled_access_terms": "*",
        "islandora/islandora_defaults": "dev-8.x-1.x",
        "islandora/openseadragon": "dev-8.x-1.x",
        "library/masonry":"*",
        "oomphinc/composer-installers-extender":"^1.1 || ^2",
        "zaporylie/composer-drupal-optimizations": "^1.1"
      },
      "require-dev":{
        "drush/drush":">=9.7",
        "phpspec/prophecy-phpunit":"^2"
      },
      "config":{
        "process-timeout":0
      },
      "extra":{
        "composer-exit-on-patch-failure":true,
        "drupal-scaffold":{
          "locations":{
            "web-root":"web/"
          }
        },
        "enable-patching":true,
        "installer-paths":{
          "web/core":[
            "type:drupal-core"
          ],
          "web/libraries/{$name}":[
            "type:drupal-library",
            "type:bower-asset",
            "type:npm-asset"
          ],
          "web/modules/contrib/{$name}":[
            "type:drupal-module"
          ],
          "web/profiles/contrib/{$name}":[
            "type:drupal-profile"
          ],
          "web/themes/contrib/{$name}":[
            "type:drupal-theme"
          ],
          "web/themes/custom/{$name}":[
            "type:drupal-theme-custom"
          ],
          "drush/contrib/{$name}":[
            "type:drupal-drush"
          ]
        },
        "installer-types":[
          "bower-asset",
          "npm-asset"
        ],
        "patchLevel":{
          "drupal/core":"-p2"
        },
        "merge-plugin":{
          "include":[
            "modules/contrib/masonry/composer.libraries.json"
          ]
        }
      },
      "repositories":{
        "drupal":{
          "type":"composer",
          "url":"https://packages.drupal.org/8"
        },
        "islandora":{
          "type":"package",
          "package":{
            "name":"islandora/islandora",
            "version":"dev-8.x-1.x",
            "type":"drupal-module",
            "source":{
              "url":"https://github.com/Islandora/islandora.git",
              "type":"git",
              "reference":"8.x-1.x"
            }
          }
        },
        "islandora_defaults":{
          "type":"package",
          "package":{
            "name":"islandora/islandora_defaults",
            "version":"dev-8.x-1.x",
            "type":"drupal-module",
            "source":{
              "url":"https://github.com/islandora/islandora_defaults.git",
              "type":"git",
              "reference":"8.x-1.x"
            }
          }
        },
        "islandora_profile":{
          "type":"package",
          "package":{
            "name":"islandora/islandora_profile",
            "version":"dev-1.x",
            "type":"drupal-profile",
            "source":{
              "url":"https://github.com/Islandora-Devops/islandora_profile.git",
              "type":"git",
              "reference":"1.x"
            }
          }

        },
        "assets":{
          "type":"composer",
          "url":"https://asset-packagist.org"
        },
        "masonry":{
          "type":"package",
          "package":{
            "type":"drupal-library",
            "name":"library/masonry",
            "version":"3.3.2",
            "dist":{
              "type":"zip",
              "url":"https://github.com/desandro/masonry/archive/v3.3.2.zip"
            }
          }
        },
        "mjordan":{
          "type":"package",
          "package":{
            "name":"mjordan/islandora_workbench_integration",
            "version":"1.0",
            "type":"drupal-module",
            "source":{
              "url":"https://github.com/mjordan/islandora_workbench_integration.git",
              "type":"git",
              "reference":"main"
            }
          }

        },
        "pdfjs":{
          "type":"package",
          "package":{
            "name":"library/pdf.js",
            "version":"2.4.456",
            "type":"drupal-library",
            "dist":{
              "url":"https://github.com/mozilla/pdf.js/releases/download/v2.4.456/pdfjs-2.4.456-dist.zip",
              "type":"zip"
            }
          }
        },
        "openseadragon":{
          "type":"package",
          "package":{
            "name":"library/openseadragon",
            "version":"2.4.2",
            "type":"drupal-library",
            "dist":{
              "url":"https://github.com/openseadragon/openseadragon/releases/download/v2.4.2/openseadragon-bin-2.4.2.zip",
              "type":"zip"
            }
          }
        }
    },
    "minimum-stability":"dev",
    "prefer-stable":false,
    "scripts":{
      "post-create-project-cmd":"rm -r -f .travis.yml .travis-ci",
      "quick-start":[
        "composer install",
        "php web/core/scripts/drupal quick-start lightning --no-interaction"
      ]
    }

}
